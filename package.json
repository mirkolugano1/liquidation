        "typescript": "^5.7.3"
    },
    "scripts": {
        "build": "tsc && npm run copy-config && npm run copy-assets",
        "copy-config": "copyfiles package.json package-lock.json dist/",
        "copy-assets": "copyfiles -u 1 src/**/*.json dist",
        "start": "npm install && node clients/webServer.js",
        "sandbox": "npx ts-node --transpileOnly src/clients/sandbox.ts",
        "webServer": "npx ts-node --transpileOnly src/clients/webServer.ts",
        "webJob": "npx ts-node --transpileOnly src/clients/webJob.ts",
        "test": "node --test --require ts-node/register tests/*.test.ts",
        "testOnly": "node --test --test-only --require ts-node/register tests/*.test.ts"
    },
    "dependencies": {
        "@types/lodash": "^4.17.15",
        "ansi-regex": "^6.1.0",
        "deep-extend": "^0.6.0",
        "dot-prop": "^9.0.0",
        "fstream": "^1.0.12",
        "hoek": "^6.1.3",
        "https-proxy-agent": "^7.0.6",
        "ip": "^2.0.1",
        "minimist": "^1.2.8",
        "npm": "^11.2.0",
        "qs": "^6.14.0",
        "tar": "^7.4.3",
        "tar-fs": "^3.0.8",
        "tough-cookie": "^5.1.2",
        "y18n": "^5.0.8"
    },
    "main": "dist/functions/*.js"
